    "version": "0.2.0",
    "configurations": [
        {
            "name": "NexusWeb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/NexusWeb",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/build/",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/build"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}